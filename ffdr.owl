    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="dct:description"/>
        <IRI>Article4</IRI>
        <Literal xml:lang="en">Data localisation requirements shall be prohibited, unless they are 
    justified on grounds of public security in compliance with the principle of proportionality.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>Article4</IRI>
        <Literal xml:lang="en">Article 4 - Prohibition of data localisation requirements</Literal>
    </AnnotationAssertion>
