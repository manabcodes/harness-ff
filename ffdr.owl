        <Literal xml:lang="en">One of the 27 member states of the European Union.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>EUMemberState</IRI>
        <Literal xml:lang="en">EU Member State</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="skos:scopeNote"/>
        <IRI>EUMemberState</IRI>
        <Literal xml:lang="en">Core class - identifies geographic entities subject to Article 4</Literal>
    </AnnotationAssertion>
